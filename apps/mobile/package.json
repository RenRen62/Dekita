  "version": "1.0.0",
  "scripts": {
    "dev:start": "run-s mock:build api:build start",
    "start": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint-staged": "lint-staged --config .lintstagedrc.js",
    "test": "jest --config jest.config.js",
    "test:cov": "jest --config jest.config.js --colors --coverage",
    "lint": "expo lint",
    "mock:build": "run-s  openapiYml2json",
    "api:build": "run-s api:clean openapi2aspida",
    "api:clean": "rimraf apis/bin",
    "openapi2aspida": "openapi2aspida -c=aspida.config.js",
    "openapiYml2json": "sh apis/mocks/json/openapiYml2json.sh",
    "eas-build-pre-install": "rm ../lambda/package.json",
    "check-changed": "CHANGED_FILES=$( (git diff --name-only HEAD && git ls-files --others --exclude-standard) | grep -E '\\.(ts|tsx)$' | grep '^apps/mobile/' || true); (if [ -n \"$CHANGED_FILES\" ]; then echo \"Processing files:\"; echo \"$CHANGED_FILES\"; echo \"$CHANGED_FILES\" | sed 's|^apps/mobile/||' | xargs eslint --cache --cache-location .eslintcache --max-warnings 0; fi) & (npx tsc --noEmit --incremental --tsBuildInfoFile .tsbuildinfo) & wait"
  },
