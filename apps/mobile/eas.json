{
  "cli": {
    "version": ">= 7.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "node": "22.16.0",
      "pnpm": "10.17.1",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "staging": {
      "node": "22.16.0",
      "pnpm": "10.17.1",
      "android": {
        "distribution": "internal",
        "buildType": "apk"
      },
      "channel": "staging",
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "staging"
      }
    },
    "production": {
      "node": "22.16.0",
      "pnpm": "10.17.1",
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "production"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "staging": {},
    "production": {}
  }
}
